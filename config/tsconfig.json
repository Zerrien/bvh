{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "../dist",
    "sourceMap": true,
    "strict": true,
    "allowJs": true,
    "paths": {
      "@src/*": ["./src/*"],
      "@resources/*": ["./public/resources/*"]
    },
    "baseUrl": "./",
  },
  "lib": [
    "webworker",
  ],
  "include": [
    "../src/**/*",
    "../public/**/*"
  ],
  "exclude": [
    "../node_modules"
  ]
}
