{
  "name": "bvh",
  "version": "0.6.3",
  "description": "BVH",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zerrien/bvh"
  },
  "scripts": {
    "pre-publish": "npm publish --dry-run",
    "build": "npm-run-all --parallel build:es6 build:es5 build:docs",
    "build:es6": "tsc",
    "build:es5": "tsc -p ./tsconfig.es5.json",
    "build:docs": "webpack",
    "start": "npm-run-all --parallel start:dev-server start:test-runner",
    "start:dev-server": "webpack-dev-server",
    "start:test-runner": "jest --watch",
    "test": "jest",
    "test:ci": "npm test -- --ci --runInBand",
    "lint": "eslint --ignore-pattern *_old.js ./src/*"
  },
  "keywords": [
    "BVH",
    "mesh",
    "ray",
    "triangle",
    "intersection",
    "webgl",
    "spatial"
  ],
  "author": "Ben Raziel & Josh Callebaut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zerrien/bvh/issues"
  },
  "homepage": "https://zerrien.github.io/bvh/public/",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "@types/three": "^0.93.4",
    "@types/webassembly-js-api": "0.0.1",
    "@types/webvr-api": "0.0.35",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "assemblyscript": "github:AssemblyScript/assemblyscript",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "jest": "^23.6.0",
    "jest-junit": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^1.9.0",
    "regenerator-runtime": "^0.12.1",
    "three": "^0.97.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.3",
    "wabt": "^1.0.6",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "whatwg-fetch": "^3.0.0"
  }
}
